{
  "rate_limits": {
    "requests_per_minute": 50,
    "input_tokens_per_minute": 40000,
    "output_tokens_per_minute": 8000
  },
  "logging": {
    "log_dir": "/tmp/ECHO2/logs",
    "log_level": "INFO",
    "file_logging": true,
    "console_logging": true
  },
  "reporting": {
    "auto_report_interval": 300,
    "save_reports": true,
    "report_directory": "/tmp/ECHO2/logs/reports"
  },
  "operation_defaults": {
    "retry_attempts": 3,
    "retry_delay": 1.5,
    "batch_size": 10
  }
}